+//=========STICKY NOTE==================================================================================================
+
+const notesContainer = document.getElementById("app");
+const addNoteButton = notesContainer.querySelector('.add-note');
+
+getNotes().forEach(note => {
+    const noteElement = createNoteElement(note.id, note.content);
+    notesContainer.insertBefore(noteElement, addNoteButton);
+});
+
+addNoteButton.addEventListener("click", () => addNote());
+
+//======================================================================================================================
+
+//henter objekter af noter fra localstorage via key 'stickynotes-notes', og hvis ik der er nogle returnerer den et tomt array
+function getNotes() {
+    return JSON.parse(localStorage.getItem('stickynotes-notes') || "[]");
+}
+
+//======================================================================================================================
+
+//gemmer noter i localstorage med key 'stickynotes-notes'
+function saveNotes(notes) {
+    localStorage.setItem('stickynotes-notes', JSON.stringify(notes));
+}
+
+//======================================================================================================================
+
+//laver et nyt textarea
+function createNoteElement(id, content, date) {
+
+    const element = document.createElement('input'); //laver et nyt html element
+
+    getDatesBetween("2022/01/01", "2023/01/01");
+
+
+    element.classList.add('note'); //det css vi har lavet til .note vil så blive brugt her
+    element.value = content;
+    element.placeholder = 'Tom note';
+
+    element.addEventListener('change', () => {
+        updateNote(id, element.value);
+    });
+
+    element.addEventListener('dblclick', () => {
+        const doDelete = confirm('er du sikker på du vil slette noten?');
+
+        if (doDelete) {
+            deleteNote(id, element);
+        }
+    })
+
+    return element;
+}
+
+//======================================================================================================================
+
+function addNote() {
+    const notes = getNotes(); //vi skal have alle noter for at tilføje en note til arrayet og så gemme det igen
+    const noteObject = {
+        id: Math.floor(Math.random() * 100000),
+        content: ""
+    };
+
+    const noteElement = createNoteElement(noteObject.id, noteObject.content);
+    notesContainer.insertBefore(noteElement, addNoteButton);
+
+    notes.push(noteObject);
+    saveNotes(notes);
+}
+
+//======================================================================================================================
+
+function updateNote(id, newContent) {
+    const notes = getNotes();
+    const targetNote = notes.filter(note => note.id == id)[0]; //filtrerer mellem noterne og finder den note der har det rigtige id, og så henter noten og ikke id'et ved [0]
+
+    targetNote.content = newContent;
+    saveNotes(notes);
+
+}
+
+//======================================================================================================================
+
+function deleteNote(id, element) {
+    const notes = getNotes().filter(note => note.id != id); //vi henter alle noter undtagen den der ønskes slettet
+
+    saveNotes(notes); //vi gemmer en ny liste af noter uden den der ønskes slettet
+    notesContainer.removeChild(element); //vi fjerner den note der ønskes slettet fra selve siden
+}
